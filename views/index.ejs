<html>
        <head>
            <title>Visualize</title>
    
            <!-- fontawesome -->
            <link rel="stylesheet" href="/font-awesome/css/font-awesome.min.css" />
            <!-- bootstrap -->
            <link rel="stylesheet" href="/bootstrap/css/bootstrap.min.css" />
            <!-- visualize css -->
            <link rel="stylesheet" href="css/visualize.css" />
        </head>
    
        <body>
            <div class="container">
                <div class="row">
                    <div class="col-md-12 mt10">
                        <div class="panel panel-primary">
                            <div class="panel-heading text-center">
                                <i class="fa fa-desktop"></i> /sav_txn/v1/circuit_log
                            </div>
                            <div class="panel-body">
                                <div class="col-md-12 text-center">
                                    <div class="col-md-3 label <%= (response.closed)?'label-success' :'label-danger'  %>">
                                        <h2 class="mt10">Closed</h2>
                                    </div>
                                    <div class="col-md-3 label <%= (response.failed)?'label-success' :'label-danger'  %> ml10" style="margin-left:10px">
                                        <h2 class="mt10">Failed</h2>
                                    </div>
                                    <div class="col-md-3 label <%= (response.tripped)?'label-success' :'label-danger'  %>" style="margin-left:10px">
                                        <h2 class="mt10">Tripped</h2>
                                    </div>
                                </div>
                                <canvas id="myChart" style="width:300"></canvas>
                            </div>
                        </div>
                    </div>
    
                </div>
            </div>
    
            <!-- jquery -->
            <script src="/jquery/jquery.min.js"></script>
            <!-- bootstrap -->
            <script src="/bootstrap/js/bootstrap.min.js"></script>
            <!-- chart.js -->
            <script src="/chart.js/Chart.bundle.min.js"></script>
            <script src="js/script.js"></script>
            
            <script src="js/retrieve.js"></script>
        </body>
    </html>